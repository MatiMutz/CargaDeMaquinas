@page "/login"
@inject IJSRuntime JsRuntime
@using SupplyChain
@inject CustomHttpClient Http
@inject NavigationManager NavManager
@inherits SupplyChain.Pages.Logins.LoginPageBase
@using System.IO
<div class="card">
    <h4 class="card-header">Login</h4>
    <div class="card-body">
        <EditForm Model="@usuarios" OnValidSubmit="OnValidSubmit">
                <div class="form-group">
                    <label>Username</label>
                    <InputText @bind-Value="@usuarios.Usuario" class="form-control" />
                </div>
                <div class="form-group">
                    <label>Password</label>
                    <InputText @bind-Value="@usuarios.Contras" type="password" class="form-control" />
                </div>
                <button disabled="" class="btn btn-primary">
                    Login
                </button>
        </EditForm>
    </div>
</div>
